    // delay decrease'
    if (attack_delay &gt; 0) { attack_delay--; }
    if (weapon_delay &gt; 0) { weapon_delay--; if (weapon_delay_j != 0) { weapon_delay_i += ((200 * weapon_trigger) / weapon_delay_j); }}
