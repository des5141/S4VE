        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// NETWORKING
if(control == 1) {
    var json_data = ds_map_create();
    ds_map_add(json_data, "id",      NN.signal_move);
    ds_map_add(json_data, "user_id", global.user_id);
    ds_map_add(json_data, "x",       x);
    ds_map_add(json_data, "y",       y);
    ds_map_add(json_data, "z",       z);
    ds_map_add(json_data, "type",    type);
    var body = json_encode(json_data);
    ds_map_destroy(json_data);
    
    nn_send_message(body);
}
</string>
